{
    "type": "MySQLNotebook",
    "version": "1.0",
    "caption": "DB Notebook",
    "content": "\\about\nUSE autoauction;\nDELIMITER //\nCREATE PROCEDURE MaxBid()\nBEGIN\n    SELECT\n        B.AutoID,\n        V.Location,\n        V.Year,\n        CONCAT('$', FORMAT(MAX(B.BidPrice), 2)) AS `Maximum Bid`\n    FROM\n        Bids B\n    INNER JOIN\n        Vehicles V ON B.AutoID = V.AutoID\n    GROUP BY\n        B.AutoID, V.Location, V.Year\n    ORDER BY\n        V.Year DESC;\nEND //\nDELIMITER ;\n\nCALL MaxBid();\nDELIMITER //\nCREATE PROCEDURE AvailableFords()\nBEGIN\n    SELECT\n        AutoID,\n        Location,\n        Year,\n        Type,\n        Mileage,\n        Vin\n    FROM\n        Vehicles\n    WHERE\n        Type LIKE 'FORD%'\n    ORDER BY\n        Year DESC;\nEND //\nDELIMITER ;\n\nCALL AvailableFords();\n\n\n\nDELIMITER //\nCREATE PROCEDURE WinnersAndLosers(IN inputAutoID INT)\nBEGIN\n    SELECT\n        V.AutoID,\n        C.LastName,\n        CONCAT('$', FORMAT(B.BidPrice, 2)) AS Bid,\n        CONCAT('$', FORMAT(\n            (SELECT MAX(BidPrice) FROM Bids WHERE AutoID = inputAutoID), 2)\n        ) AS `Maximum Bid`,\n        CASE WHEN B.BidPrice = (\n            SELECT MAX(BidPrice) FROM Bids WHERE AutoID = inputAutoID\n        ) THEN 'Yes' ELSE 'No' END AS `Won Bid?`\n    FROM\n        Bids B\n    INNER JOIN\n        Customers C ON B.CustomerID = C.CustomerID\n    RIGHT JOIN\n        Vehicles V ON B.AutoID = V.AutoID\n    WHERE\n        V.AutoID = inputAutoID;\nEND //\nDELIMITER ;\nCALL WinnersAndLosers(1); \nCALL WinnersAndLosers(15); \nCALL WinnersAndLosers(5); \nDELIMITER //\nCREATE PROCEDURE AvailableAuto()\nBEGIN\n    SELECT\n        AutoID,\n        Location,\n        Year,\n        Type,\n        Mileage,\n        Vin\n    FROM\n        Vehicles\n    ORDER BY\n        Year DESC;\nEND //\nDELIMITER ;\nCALL AvailableAuto();\nDELIMITER //\nCREATE PROCEDURE add_customer(\n    IN inputCustomerID INT,\n    IN inputLastName VARCHAR(50),\n    IN inputFirstName VARCHAR(50),\n    IN inputAddress VARCHAR(255),\n    IN inputCity VARCHAR(50),\n    IN inputState VARCHAR(2),\n    IN inputZip INT,\n    IN inputTelephone VARCHAR(15),\n    IN inputEmail VARCHAR(100)\n)\nBEGIN\n    INSERT INTO Customers (\n        CustomerID,\n        LastName,\n        FirstName,\n        Address,\n        City,\n        State,\n        Zip,\n        Telephone,\n        Email\n    ) VALUES (\n        inputCustomerID,\n        inputLastName,\n        inputFirstName,\n        inputAddress,\n        inputCity,\n        inputState,\n        inputZip,\n        inputTelephone,\n        inputEmail\n    );\nEND //\nDELIMITER ;\n\nUSE autoauction\nDELIMITER //\nCREATE TRIGGER AutoBidTrigger\nAFTER INSERT ON Customers\nFOR EACH ROW\nBEGIN\n    DECLARE randomAutoID INT;\n    DECLARE randomDate DATE;\n    DECLARE randomPrice DECIMAL(10, 2);\n\n    SET randomAutoID = FLOOR(1 + RAND() * 25);\n    SET randomDate = DATE_ADD('2010-01-01', INTERVAL FLOOR(RAND() * 5023) DAY);\n    SET randomPrice = 1000 + (RAND() * 19000);\n\n    INSERT INTO Bids (CustomerID, AutoID, BidDate, BidPrice)\n    VALUES (NEW.CustomerID, randomAutoID, randomDate, randomPrice);\nEND //\nDELIMITER ;\nCALL add_customer(111, 'Smith', 'John', '123 Main St', 'Anytown', 'CA', '12345', '(123) 456-7890', 'john@example.com');\nSELECT * FROM customers;\nDELIMITER //\nCREATE PROCEDURE add_available_auto(\n    IN inputAutoID INT,\n    IN inputLocation VARCHAR(255),\n    IN inputYear INT,\n    IN inputType VARCHAR(255),\n    IN inputMileage INT,\n    IN inputVin VARCHAR(255)\n)\nBEGIN\n    INSERT INTO Vehicles (\n        AutoID,\n        Location,\n        Year,\n        Type,\n        Mileage,\n        Vin\n    ) VALUES (\n        inputAutoID,\n        inputLocation,\n        inputYear,\n        inputType,\n        inputMileage,\n        inputVin\n    );\nEND //\nDELIMITER ;\n\nCALL add_available_auto(26, 'D123', 2023, 'TOYOTA CAMRY', 5, '1234567890');\nUSE autoauction;\nSELECT * FROM bids;\n\nDROP TRIGGER IF EXISTS check_bid;\nDELIMITER //\n\nCREATE TRIGGER check_bid\nBEFORE INSERT ON bids\nFOR EACH ROW\nBEGIN\n    DECLARE valid_term INT;\n    DECLARE this_date DATE;\n    DECLARE earliest_date DATE;\n    DECLARE this_price INT ;\n    DECLARE biggest_price INT ;\n    DECLARE customer_exists INT;\n    DECLARE auto_exists INT; -- 用于检查CustomerID是否存在的变量\n    \n    SET valid_term = 30;\n    SET this_date = NEW.BidDate;\n    SET this_price = NEW.BidPrice;\n\n    SELECT COUNT(*) INTO auto_exists\n    FROM vehicles\n    WHERE AutoID = NEW.AutoID;\n   \n    SELECT COUNT(*) INTO customer_exists\n    FROM customers\n    WHERE CustomerID = NEW.CustomerID;\n\n    IF customer_exists = 0 THEN\n        SIGNAL SQLSTATE '45000'\n        SET MESSAGE_TEXT = 'Customer not found';\n    ELSEIF auto_exists = 0 THEN\n        SIGNAL SQLSTATE '45000'\n        SET MESSAGE_TEXT = 'Auto not found';\n    ELSE\n        -- 如果CustomerID存在，继续执行其他检查\n        SELECT MIN(BidDate) INTO earliest_date\n        FROM bids\n        WHERE AutoID = NEW.AutoID;\n\n        SELECT MAX(BidPrice) INTO biggest_price\n        FROM bids\n        WHERE AutoID = NEW.AutoID;\n\n        IF earliest_date IS NOT NULL AND DATEDIFF(this_date, earliest_date) > valid_term THEN\n            SIGNAL SQLSTATE '45000'\n            SET MESSAGE_TEXT = 'Bid is overdue';\n        END IF;\n\n        IF this_price < biggest_price THEN\n            SIGNAL SQLSTATE '45000'\n            SET MESSAGE_TEXT = 'Price is lower than the highest';\n        END IF;\n    END IF;\nEND;\n//\nDELIMITER ;\n\nDROP TRIGGER IF EXISTS BidCheck;\nUSE autoauction;\n\nUSE autoauction\nDELIMITER //\nCREATE TRIGGER AutoBidTrigger\nAFTER INSERT ON Customers\nFOR EACH ROW\nBEGIN\n    DECLARE randomAutoID INT;\n    DECLARE randomDate DATE;\n    DECLARE randomPrice DECIMAL(10, 2);\n\n    SET randomAutoID = FLOOR(1 + RAND() * 25);\n    SET randomDate = DATE_ADD('2009-01-01', INTERVAL FLOOR(RAND() * 45) DAY);\n    SET randomPrice = 1000 + (RAND() * 19000);\n\n    INSERT INTO Bids (CustomerID, AutoID, BidDate, BidPrice)\n    VALUES (NEW.CustomerID, randomAutoID, randomDate, randomPrice);\nEND //\nDELIMITER ;\nINSERT INTO bids (CustomerID, AutoID, BidDate, BidPrice)\nVALUE (112, 1, '2009-01-25', 10000)\nINSERT INTO bids (CustomerID, AutoID, BidDate, BidPrice)\nVALUE (119, 1, '2009-01-25', 10000)\nDROP PROCEDURE IF EXISTS GetAutoIDs;\nDELIMITER //\nCREATE PROCEDURE GetAutoIDs()\nBEGIN\n    CREATE TEMPORARY TABLE IF NOT EXISTS temp_auto_ids (AutoID INT);\n\n    INSERT INTO temp_auto_ids\n    SELECT AutoID\n    FROM bids\n    WHERE ABS(DATEDIFF(NEW.BidDate, BidDate)) > 30;\n\n    -- SELECT * FROM temp_auto_ids;\n    DROP TEMPORARY TABLE IF EXISTS temp_auto_ids;\nEND;\n\nDROP TRIGGER IF EXISTS win_check;\nDELIMITER //\n\nCREATE TRIGGER win_check\nAFTER INSERT ON bids\nFOR EACH ROW\nBEGIN\n    DECLARE valid_term INT;\n    DECLARE this_date DATE;\n\n    SET valid_term = 30;\n    SET this_date = NEW.BidDate; -- NEW指允许访问触发行的值\n\n    IF EXISTS (\n        SELECT 1\n        FROM bids\n        WHERE DATEDIFF(this_date, BidDate) > valid_term\n    ) THEN\n        SELECT *\n        FROM bids\n        WHERE DATEDIFF(this_date, BidDate) > valid_term;\n        -- 或者根据需要进行其他操作，比如将满足条件的行插入到另一个表中等\n    END IF;\nEND;\n//\n\nDELIMITER ;\nDELETE FROM bids\nWHERE BidDate='2009-02-07';\nINSERT INTO bids (CustomerID, AutoID, BidDate, BidPrice)\nVALUE (112, 25, '2009-02-07', 10000);\nSELECT * FROM bids;\nSELECT * FROM customers;\nDELIMITER //\n\nCREATE TRIGGER check_customerID_exists\nBEFORE INSERT ON customers\nFOR EACH ROW\nBEGIN\n    DECLARE customer_exists INT;\n\n    -- 检查新插入的 CustomerID 是否已经存在\n    SELECT COUNT(*) INTO customer_exists\n    FROM customers\n    WHERE CustomerID = NEW.CustomerID;\n\n    IF customer_exists > 0 THEN\n        SIGNAL SQLSTATE '45000'\n        SET MESSAGE_TEXT = 'CustomerID already exists';\n    END IF;\nEND;\n//\n\nDELIMITER ;\n\nINSERT INTO Customers (\n        CustomerID,\n        LastName,\n        FirstName,\n        Address,\n        City,\n        State,\n        Zip,\n        Telephone,\n        Email\n    ) VALUES (111, 'Smith', 'John', '123 Main St', 'Anytown', 'CA', '12345', '(123) 456-7890', 'john@example.com');\nSHOW TRIGGERS;\nDROP TRIGGER AutoBidTrigger;\nDELETE FROM customers WHERE CustomerID = 114;\n",
