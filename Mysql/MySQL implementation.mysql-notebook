                            "0": "BidCheckTrigger",
                            "1": "INSERT",
                            "2": "bids",
                            "3": "BEGIN\n    DECLARE earliestBidDate DATE;\n    DECLARE currentMaxBid DECIMAL(10, 2);\n    DECLARE currentDate DATE;\n    DECLARE customerIdWithMaxBid INT;\n    DECLARE autoIdWithMaxBid INT;\n    \n    -- 获取当前竞标的AutoID和BidDate\n    SET autoIdWithMaxBid = NEW.AutoID;\n    SET currentDate = NEW.BidDate;\n\n    -- 获取该AutoID的最早竞标日期和当前最大竞标价格\n    SELECT MIN(BidDate), MAX(BidPrice) INTO earliestBidDate, currentMaxBid\n    FROM Bids\n    WHERE AutoID = autoIdWithMaxBid;\n\n    -- 检查是否超过一个月\n    IF DATEDIFF(currentDate, earliestBidDate) > 30 THEN\n        -- 打印消息，表明竞标日期已经过期\n        SET @message1 = CONCAT('Bid date ', DATE_FORMAT(currentDate, '%Y-%m-%d'), ' is overdue.');\n        \n        -- 检查是否有竞标价格低于当前最大竞标价格\n        IF NEW.BidPrice < currentMaxBid THEN\n            -- 打印消息，表明竞标价格低于当前最大竞标价格\n            SET @message2 = CONCAT('Bid ', NEW.BidPrice, ' is less than current max bid ', currentMaxBid);\n        END IF;\n        \n        -- 检查是否有竞标超过一个月并更新数据库\n        IF DATEDIFF(currentDate, earliestBidDate) > 30 THEN\n            -- 找到最大竞标价格对应的CustomerID\n            SELECT CustomerID INTO customerIdWithMaxBid\n            FROM Bids\n            WHERE AutoID = autoIdWithMaxBid AND BidPrice = currentMaxBid\n            LIMIT 1;\n            \n            -- 更新数据库，标记该客户为竞标赢家\n            UPDATE Bids\n            SET WonBid = 1\n            WHERE AutoID = autoIdWithMaxBid AND CustomerID = customerIdWithMaxBid;\n            \n            -- 打印消息，表明该客户赢得了竞标\n            SET @message3 = CONCAT('Customer ', customerIdWithMaxBid, ' wins AutoID ', autoIdWithMaxBid);\n        END IF;\n        \n        -- 将消息插入到TriggerResults表格中\n        INSERT INTO TriggerResults (Message) VALUES (@message1);\n        INSERT INTO TriggerResults (Message) VALUES (@message2);\n        INSERT INTO TriggerResults (Message) VALUES (@message3);\n    END IF;\nEND",
